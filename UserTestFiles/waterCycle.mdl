water on cover  = A FUNCTION OF( condensation)
	~	
	~	<[VIEW]>: View 1 <[DESCRIPTION]>:
	|

k 2  = A FUNCTION OF( precipitation)
	~	
	~<[VIEW]>: vista cambiada <[DESCRIPTION]>:		|

water in pan  = A FUNCTION OF( -evaporation)
	~	
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

precipitation  = A FUNCTION OF( k 2)
	~	
	~<[VIEW]>: vista cambiada <[DESCRIPTION]>:		|

water in air= INTEG (
	evaporation-condensation-water vapor leak,
		0)
	~	
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

water vapor leak=
	0
	~	 [0,1,0.1]
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

k 1=
	0.5
	~	 [0,1,0.1]
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

condensation=
	k 1*water in air
	~	
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

amount of evaporation per degree=
	0.05
	~	 [0,0.2,0.01]
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

evaporation=
	water temperature*amount of evaporation per degree
	~	
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

total water=
	water in air+water in pan+water on cover
	~	
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

water temperature=
	100
	~	 [0,100,1]
	~<[VIEW]>: View 1 <[DESCRIPTION]>:		|

