{UTF-8}
sacrificios=
	(Cerdos para mercado/periodo de engorde)+PULSE(6,6)*10000
	~	cerdos/mes
	~	Consideraremos una vida media de 6 meses, por lo que en cada periodo \
		sacrificaremos la sexta parte del nivel de Cerdos para el mercado. \
		Añadimos un Test para ver el efecto de una leve variación en los \
		sacrificios.
	|

efecto de la cobertura en el precio = WITH LOOKUP (
	cobertura/cobertura deseada,
		((0.4,1.5),(0.9,1.2),(1,1),(1.6,0.9),(1.8,0.8),(2,0.5) ))
	~	Dmnl
	~		|

precio del embutido=
	smooth(precio del cerdo+margen,tiempo de retraso)
	~	euros/kilo
	~		|

tiempo de retraso=
	3
	~	mes
	~		|

ajuste=
	(cerdas de cria deseadas-Cerdas de cria)/tiempo de ajuste
	~	cerdos/mes
	~	Se considera que el ajuste entre los Cerdos de Cria deseadis y los reales \
		tarda 3 meses. Para ello se utiliza la función de retardo SMOOTH con una \
		constante de tiempo de 3.
	|

Cerdas de cria= INTEG (
	ajuste,
		500000)
	~	cerdos
	~	Llamadas "marranas" son las cerdas destinadas a la crianza. Por \
		simplicidad consideraremos que no se aprovecha su carne para embutidos, \
		aunque en realidad si se hace.
	|

cerdas de cria normal=
	500000
	~	cerdos
	~		|

cerdas de cria deseadas=
	cerdas de cria normal*efecto del precio en la cria
	~	cerdos
	~	Son función del precio del cerdo. De forma que cuando este se halla en 3 \
		€ (euros) la cantidad de cerdos de cria deseados es de 500.000, tal y \
		como recogemos en la Tabla 3.
	|

Cerdos para mercado= INTEG (
	nacimientos-sacrificios,
		4.5e+06)
	~	cerdos
	~		|

crias al mes=
	1.5
	~	1/mes
	~	Tomaremos una tasa de natalidad de 18 lechones a año, lo que significa \
		que cada cerda de cria tiene 1,5 crias al mes
	|

produccion=
	sacrificios*peso por cerdo*ratio util del cerdo
	~	kilos/mes
	~		|

precio del cerdo=
	efecto de la cobertura en el precio*precio normal del cerdo
	~	euros/kilo
	~		|

efecto del precio en la cria = WITH LOOKUP (
	precio del cerdo/precio normal del cerdo,
		([(0,0)-(10,10)],(0,0.2),(0.3,0.4),(1,1),(2,1.2),(3,1.8) ))
	~	Dmnl
	~		|

precio normal del cerdo=
	3
	~	euros/kilo
	~		|

tiempo de ajuste=
	3
	~	mes
	~		|

nacimientos=
	Cerdas de cria*crias al mes
	~	cerdos/mes
	~	Tomaremos una tasa de natalidad de 18 lechones a año, lo que significa \
		que cada cerda de cria tiene 1,5 crias al mes
	|

periodo de engorde=
	6
	~	mes
	~		|

cobertura deseada=
	0.5
	~	mes
	~		|

consumo por persona=
	consumo normal*efecto del precio en el consumo
	~	kilos/(mes*persona)
	~		|

efecto del precio en el consumo  = WITH LOOKUP (
	precio relativo,
		([(0,0)-(10,10)],(0.5,1.5),(1,1),(1.5,0.9),(2,0.75) ))
	~	Dmnl
	~		|

ratio util del cerdo=
	0.8
	~	Dmnl
	~	Porcentaje del peso del cerdo que se aprovecha para embutidos.
	|

cobertura=
	Existencias de embutidos en tocinerias/consumo medio
	~	mes
	~	Numero de meses que se puede atender al consumo medio con las Existencias \
		de embutidos en las tocinerias.
	|

consumo =
	poblacion*consumo por persona
	~	kilos/mes
	~		|

consumo medio =
	poblacion*consumo normal
	~	kilos/mes
	~		|

consumo normal=
	1.5
	~	kilos/persona/mes
	~		|

margen=
	7
	~	euros/kilo
	~		|

peso por cerdo =
	100
	~	kilos/cerdos
	~		|

poblacion =
	4e+07
	~	persona
	~	Poblacion española de 40.000.000 aprox.
	|

precio normal del embutido=
	10
	~	euros/kilo
	~		|

precio relativo =
	precio del embutido/precio normal del embutido
	~	Dmnl
	~		|

Existencias de embutidos en tocinerias= INTEG (
	+produccion-consumo,
		3e+07)
	~	kilos
	~		|

********************************************************
	.Control
********************************************************~
		Simulation Control Paramaters
	|

FINAL TIME  = 48
	~	mes
	~	The final time for the simulation.
	|

INITIAL TIME  = 0
	~	mes
	~	The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP
	~	mes
	~	The frequency with which output is stored.
	|

TIME STEP  = 1
	~	mes
	~	The time step for the simulation.
	|

