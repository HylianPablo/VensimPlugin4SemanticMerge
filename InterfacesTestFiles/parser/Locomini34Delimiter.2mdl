"percention of shortage of primary resources for R1 (pr)"[Primary Resources]= INTEG (
	increment perception shortage primaries[Primary Resources]
	-decrement perception shortage primaries[Primary Resources],
		INITIAL PERCEPTION SHORTAGE PRIMARIES[Primary Resources])
	~	
	~		|

dydt[Primary Resources]=
	(1/0.5)*(u[Primary Resources]-y[Primary Resources])
	~	
	~<[VIEW]>: Perception of shortage <[DESCRIPTION]>:	esto es para resolver el lazo algebraico es un sistema de primer orden de \
		constante de tiempo (1/2) (a?os)
	|

decrement perception shortage primaries[Primary Resources]=
	"percention of shortage of primary resources for R1 (pr)"[Primary Resources]*(1/6)
	~	
	~<[VIEW]>: Perception of shortage <[DESCRIPTION]>:		|

"shortage of primary resources (pr)"[Primary Resources]=
	ZIDZ( (primary resources demanded to global market[R1,Primary Resources]+primary resources demanded to global market\
		[R2,Primary Resources])  - ("primary resources allocated to R1 (pr)"[Primary Resources
	]+"primary resources allocated to R2 (pr)"[Primary Resources]),(primary resources demanded to global market\
		[R1,Primary Resources]+primary resources demanded to global market[R2,Primary Resources\
		]))
	~	
	~		|

"excedent of primary resources (pr)"[Primary Resources]=
	y[Primary Resources]
	~	
	~	si la abundancia es mayor que cero el excedente es la diferencia entre la \
		demanda y la oferta mundial, si es menor que cero hay shortage y se toma \
		negativo y es la diferencia entre el resucor primario allocated a la \
		region y la demanda
	|

increment perception shortage primaries[Primary Resources]=
	"shortage of primary resources (pr)"[Primary Resources]*coefp0
	~	
	~<[VIEW]>: Perception of shortage <[DESCRIPTION]>:		|

u[Primary Resources]=
	IF THEN ELSE( "abundance of primary resources(pr)"[Primary Resources]>0 , "abundance of primary resources(pr)"\
		[Primary Resources
	] , -"shortage of primary resources (pr)"[Primary Resources] )
	~	
	~<[VIEW]>: Perception of shortage <[DESCRIPTION]>:		|

y 0[Regions,Final Resources,Primary Resources]= INTEG (
	dydt 0[Regions,Final Resources,Primary Resources],
		0)
	~	
	~<[VIEW]>: Perception of shortage <[DESCRIPTION]>:		|

"abundance of primary resources(pr)"[Primary Resources]=
	ZIDZ ("Primary Resources Available to Global Market(pr)"[Primary Resources]-primary resources demanded to global market regions agregated\
		[Primary Resources],primary resources demanded to global market regions agregated[Primary Resources\
		])
	~	
	~	la abundancia va los con recursos globales, todo en el mercado global \
		antes de repartirse entre regiones, nos sirve para ver la abundancia \
		cuando hay excedente de una fuente primaria
	|

cereals to biomass shares R2[Final Resources]=
	IF THEN ELSE( "posibility of share exchange R2 (ex,fr)"[cereals biomass,Final Resources\
		]=1 , 
	
	coeff share exchange R2*"posibility of share exchange R2 (ex,fr)"[cereals biomass,Final Resources\
		]*ABS("excedent of primary resources (pr)"[p7biomass]-"excedent of primary resources (pr)"
	[p4cereals])*shares finals from p4cereals in R2[Final Resources]*(1-shares finals from p7biomass in R2\
		[Final Resources]),
	
	coeff share exchange R2*"posibility of share exchange R2 (ex,fr)"[cereals biomass,Final Resources\
		]* ABS("excedent of primary resources (pr)"[p7biomass]-
	"excedent of primary resources (pr)"[p4cereals])*shares finals from p7biomass in R2[\
		Final Resources]*(1-shares finals from p4cereals in R2
	[Final Resources])
	 )
	~	
	~<[VIEW]>: Increments of shares F from P in R2 <[DESCRIPTION]>:	"posibility of share exchange (ex,fr)"[cereals biomass,Final Resources]*(IF THEN \
		ELSE( "excedent of primary resources in R1 (pr)"[p7biomass]>0 ,
		annual share exchange*shares finals from p4cereals in R1[Final Resources]*(1-shares \
		finals from p7biomass in R1[Final Resources
		]),
		
		IF THEN ELSE( "excedent of primary resources in R1 (pr)"[p4cereals]<"excedent of \
		primary resources in R1 (pr)"[p7biomass] ,
		 
		
		annual share exchange*shares finals from p4cereals in R1[Final Resources]*(1-shares \
		finals from p7biomass in R1[Final Resources
		]) , 
		
		annual share exchange*(1-shares finals from p4cereals in R1[Final Resources])*shares \
		finals from p7biomass in R1[Final Resources
		]
		 )
		))
	|

