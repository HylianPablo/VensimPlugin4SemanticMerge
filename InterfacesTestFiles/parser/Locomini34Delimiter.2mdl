		[R1,p2gas,Final Resources]*SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p7biomass,Final Resources\
		]>0:AND:"available infrastructure finals from primaries (R,pr,fr)"[R1,Final Resources\
		,p7biomass]>0:AND:"excedent of primary resources (pr)"[p7biomass]>="excedent of primary resources (pr)"\
		[p2gas],1,
	
	IF THEN ELSE( "excedent of primary resources (pr)"[p7biomass]<0:AND:SUITABILITY OF FINAL TO PRIMARY RESOURCES\
		[R1,p7biomass,Final Resources]*SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p2gas,Final Resources\
		]>0:AND:
	"available infrastructure finals from primaries (R,pr,fr)"[R1,Final Resources,p2gas]\
		>0:AND:
	"excedent of primary resources (pr)"[p2gas]>"excedent of primary resources (pr)"[p7biomass\
		],
	-1,
	
	0)) ~~|
"posibility of share exchange R1 (ex,fr)"[cereals pulses,Final Resources]=
	IF THEN ELSE("excedent of primary resources (pr)"[p4cereals]<0:AND:SUITABILITY OF FINAL TO PRIMARY RESOURCES\
		[R1,p4cereals,Final Resources]*SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p5pulses\
		,Final Resources]>0:AND:"available infrastructure finals from primaries (R,pr,fr)"[\
		R1,Final Resources,p5pulses]>0:AND:"excedent of primary resources (pr)"[p5pulses]>=\
		"excedent of primary resources (pr)"[p4cereals],1,
	
	IF THEN ELSE( "excedent of primary resources (pr)"[p5pulses]<0:AND:SUITABILITY OF FINAL TO PRIMARY RESOURCES\
		[R1,p5pulses,Final Resources]*SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p4cereals\
		,Final Resources]>0:AND:
	"available infrastructure finals from primaries (R,pr,fr)"[R1,Final Resources,p4cereals\
		]>0:AND:
	"excedent of primary resources (pr)"[p4cereals]>"excedent of primary resources (pr)"\
		[p5pulses],
	-1,
	
	0)) ~~|
"posibility of share exchange R1 (ex,fr)"[cereals biomass,Final Resources]=
	IF THEN ELSE("excedent of primary resources (pr)"[p4cereals]<0:AND:SUITABILITY OF FINAL TO PRIMARY RESOURCES\
		[R1,p4cereals,Final Resources]*SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p7biomass\
		,Final Resources]>0:AND:"available infrastructure finals from primaries (R,pr,fr)"[\
		R1,Final Resources,p7biomass]>0:AND:"excedent of primary resources (pr)"[p7biomass]\
		>="excedent of primary resources (pr)"[p4cereals],1,
	
	IF THEN ELSE("excedent of primary resources (pr)"[p7biomass]<0:AND: SUITABILITY OF FINAL TO PRIMARY RESOURCES\
		[R1,p7biomass,Final Resources]*SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p4cereals\
		,Final Resources]>0:AND:
	"available infrastructure finals from primaries (R,pr,fr)"[R1,Final Resources,p4cereals\
		]>0:AND:
	"excedent of primary resources (pr)"[p4cereals]>"excedent of primary resources (pr)"\
		[p7biomass],
	-1,
	
	0)) ~~|
"posibility of share exchange R1 (ex,fr)"[pulses biomass,Final Resources]=
	IF THEN ELSE("excedent of primary resources (pr)"[p5pulses]<0:AND:SUITABILITY OF FINAL TO PRIMARY RESOURCES\
		[R1,p5pulses,Final Resources]*SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p7biomass\
		,Final Resources]>0:AND:"available infrastructure finals from primaries (R,pr,fr)"[\
		R1,Final Resources,p7biomass]>0:AND:"excedent of primary resources (pr)"[p7biomass]\
		>="excedent of primary resources (pr)"[p5pulses],1,
	
	IF THEN ELSE( "excedent of primary resources (pr)"[p7biomass]<0:AND:SUITABILITY OF FINAL TO PRIMARY RESOURCES\
		[R1,p7biomass,Final Resources]*SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p5pulses\
		,Final Resources]>0:AND:
	"available infrastructure finals from primaries (R,pr,fr)"[R1,Final Resources,p5pulses\
		]>0:AND:
	"excedent of primary resources (pr)"[p5pulses]>"excedent of primary resources (pr)"[\
		p7biomass],
	-1,
	
	0))
	~	
	~	"abailable infrastructure finals from primaries (R,pr,fr)"[R1,Final \
		Resources,p2gas]:AND:,
		SUITABILITY OF FINAL TO PRIMARY RESOURCES[R1,p1oil,Final Resources]:AND:SUITABILITY \
		OF FINAL TO PRIMARY RESOURCES[R1,p2gas,Final Resources]:AND:"excedent of \
		primary resources in R1 (pr)"[p1oil]>0,
		1 , 0 )
	|

gas to pulses shares R2[Final Resources]=
	IF THEN ELSE( "posibility of share exchange R2 (ex,fr)"[gas pulses,Final Resources]=\
		1 , 
	
	coeff share exchange R2*"posibility of share exchange R2 (ex,fr)"[gas pulses,Final Resources\
		]*ABS("excedent of primary resources (pr)"[p5pulses]-"excedent of primary resources (pr)"
	[p2gas])*shares finals from p2gas in R2[Final Resources]*(1-shares finals from p5pulses in R2\
		[Final Resources]),
	
	coeff share exchange R2*"posibility of share exchange R2 (ex,fr)"[gas pulses,Final Resources\
		]* ABS("excedent of primary resources (pr)"[p5pulses]-
	"excedent of primary resources (pr)"[p2gas])*shares finals from p5pulses in R2[Final Resources\
		]*(1-shares finals from p2gas in R2
	[Final Resources])
	 )
	~	
	~<[VIEW]>: Increments of shares F from P in R2 <[DESCRIPTION]>:	"posibility of share exchange (ex,fr)"[gas pulses,Final Resources]*(IF THEN ELSE( \
		"excedent of primary resources in R1 (pr)"[p5pulses]>0 ,
		annual share exchange*shares finals from p2gas in R1[Final Resources]*(1-shares \
		finals from p5pulses in R1[Final Resources
		]),
		
		IF THEN ELSE( "excedent of primary resources in R1 (pr)"[p2gas]<"excedent of primary \
		resources in R1 (pr)"[p5pulses] ,
		 
		
		annual share exchange*shares finals from p2gas in R1[Final Resources]*(1-shares \
		finals from p5pulses in R1[Final Resources
		]) , 
		
		annual share exchange*(1-shares finals from p2gas in R1[Final Resources])*shares \
		finals from p5pulses in R1[Final Resources
		]
		 )
		))
	|

gas to biomass hares R2[Final Resources]=
	IF THEN ELSE( "posibility of share exchange R2 (ex,fr)"[gas biomass,Final Resources]\
		=1 , 
	
	coeff share exchange R2*"posibility of share exchange R2 (ex,fr)"[gas biomass,Final Resources\
		]*ABS("excedent of primary resources (pr)"[p7biomass]-"excedent of primary resources (pr)"
	[p2gas])*shares finals from p2gas in R2[Final Resources]*(1-shares finals from p7biomass in R2\
		[Final Resources]),
	
	coeff share exchange R2*"posibility of share exchange R2 (ex,fr)"[gas biomass,Final Resources\
		]* ABS("excedent of primary resources (pr)"[p7biomass]-
	"excedent of primary resources (pr)"[p2gas])*shares finals from p7biomass in R2[Final Resources\
		]*(1-shares finals from p2gas in R2
	[Final Resources])
	 )
	~	
	~<[VIEW]>: Increments of shares F from P in R2 <[DESCRIPTION]>:	"posibility of share exchange (ex,fr)"[gas biomass,Final Resources]*(IF THEN ELSE( \
		"excedent of primary resources in R1 (pr)"[p7biomass]>0 ,
		annual share exchange*shares finals from p2gas in R1[Final Resources]*(1-shares \
		finals from p7biomass in R1[Final Resources
		]),
		
		IF THEN ELSE( "excedent of primary resources in R1 (pr)"[p2gas]<"excedent of primary \
		resources in R1 (pr)"[p7biomass] ,
		 
		
		annual share exchange*shares finals from p2gas in R1[Final Resources]*(1-shares \
		finals from p7biomass in R1[Final Resources
		]) , 
		
		annual share exchange*(1-shares finals from p2gas in R1[Final Resources])*shares \
		finals from p7biomass in R1[Final Resources
		]
		 )
		))
	|

shares finals from p2gas in R2[Final Resources]= INTEG (
	oil to gas shares R2[Final Resources]-gas to biomass hares R2[Final Resources]-gas to cereals shares R2\
		[Final Resources]-gas to pulses shares R2[Final Resources],
		"INITIAL SHARES FINALS FROM PRIMARIES (R,pr,fr) 0"[R2,p2gas,Final Resources])
