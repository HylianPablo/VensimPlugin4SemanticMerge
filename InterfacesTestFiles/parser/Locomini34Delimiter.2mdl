"income to food per person (R)"[Regions]=
	"income of households to food (R)"[Regions]/"Population (R)"[Regions]
	~	
	~		|

"Afroffod System matrix from imncome (R,d) 0"[Regions,Final Resources,Diet products]=
	IF THEN ELSE(  "income to food per person (R)"[Regions]<Income diet1[Regions], "Agrofood System Matrix 1 GDP(R,fr,d)"
	[Regions,Final Resources,Diet products],
	
	IF THEN ELSE( "income to food per person (R)"[Regions]<Income diet 2[Regions],
	
	"Agrofood System Matrix 1 GDP(R,fr,d)"[Regions,Final Resources,Diet products]*(1-(("income to food per person (R)"\
		[Regions]-Income diet1[Regions])/(Income diet 2[Regions
	]-Income diet1[Regions])))+   
	
	"Agrofood System Matrix 2 (R,fr,d)"[Regions,Final Resources,Diet products]*(("income to food per person (R)"\
		[Regions]-Income diet1[Regions])/(Income diet 2[Regions
	]-Income diet1[Regions])), 
	
	"Agrofood System Matrix 2 (R,fr,d)"[Regions,Final Resources,Diet products]    ))
	~	
	~		|

"Diet from income(R,d)"[Regions,Diet products]=
	IF THEN ELSE(  "income to food per person (R)"[Regions]<Income diet1[Regions], Diet 1\
		[Regions,Diet products],
	
	IF THEN ELSE( "income to food per person (R)"[Regions],
	
	Diet 1[Regions,Diet products]*(1-(("income to food per person (R)"[Regions]-Income diet1\
		[Regions])/(Income diet 2[Regions
	]-Income diet1[Regions])))+   
	
	Diet 2[Regions,Diet products]*(("income to food per person (R)"[Regions]-Income diet1\
		[Regions])/(Income diet 2[Regions
	]-Income diet1[Regions])), 
	
	Diet 2[Regions,Diet products]    ))
	~	
	~		|

