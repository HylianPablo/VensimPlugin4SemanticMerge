	Renewable_water_resources_W/NUMBER_OF_REGIONS
	~	km3
	~<[VIEW]>: land_and_water.exogenous_inputs <[DESCRIPTION]>:		|

Renewable_water_resources_W=
	GET_DIRECT_CONSTANTS('/model_parameters/energy/energy.xlsx', 'World', 'Renewable_water_resources'\
		)
	~	km3
	~<[VIEW]>: land_and_water.exogenous_inputs <[DESCRIPTION]>:		|

replacement_capacity_PHS[REGIONS_I]=
	IF_THEN_ELSE(Time<2015,0,replacement_rate_PHS[REGIONS_I]*wear_PHS[REGIONS_I])
	~	TW/Year
	~<[VIEW]>: energy-electricity-PHS_generation <[DESCRIPTION]>:	Replacement capacity PHS.
	|

replacement_capacity_RES_elec[RES_elec, REGIONS_I]=
	IF_THEN_ELSE(Time<2015,0,replacement_rate_RES_elec[RES_elec,REGIONS_I]*Decom_RES_elec\
		[RES_elec,REGIONS_I])
	~	TW/Year
	~<[VIEW]>: energy-electricity-ELEC RES.generation <[DESCRIPTION]>:	Annual replacement of RES infrastructure for electricity generation by \
		technology. It is assumed that the step of planning the replaced \
		infrastructure can be done while the infraestructure to be replaced is \
		still under operation. For replaced infraestructures, the construction \
		time should be smaller than for new infaestructures, however we compensate \
		for this assuming that the demantling time is included in construction \
		time for replaced infrastructure.
	|

replacement_CTL[REGIONS_I]=
	IF_THEN_ELSE(Time<2015,0,
	IF_THEN_ELSE(SWITCH_ACTIVATION_CRASH_PROGRAMME_CTL_SP[REGIONS_I]=0,0,
	IF_THEN_ELSE(check_liquids[REGIONS_I]<-0.0001, "constrain_liquids_exogenous_growth?"\
		[REGIONS_I]*wear_CTL[REGIONS_I], wear_CTL[REGIONS_I])))*scarcity_conv_oil[REGIONS_I\
		]
	~	EJ/Year
	~<[VIEW]>: energy.GTL_and_CTL_supply <[DESCRIPTION]>:	Replacement of CTL.
	|

replacement_GTL[REGIONS_I]=
	IF_THEN_ELSE(Time<2015,0,
	IF_THEN_ELSE(SWITCH_ACTIVATION_CRASH_PROGRAMME_GTL_SP[REGIONS_I]=0,0,
	IF_THEN_ELSE(check_liquids[REGIONS_I]<-0.0001, "constrain_liquids_exogenous_growth?"\
		[REGIONS_I]*wear_GTL[REGIONS_I], wear_GTL[REGIONS_I])))*scarcity_conv_oil[REGIONS_I\
		]
	~	EJ/Year
	~<[VIEW]>: energy.GTL_and_CTL_supply <[DESCRIPTION]>:	Replacement of GTL.
	|

replacement_nuclear_capacity[REGIONS_I]=
	IF_THEN_ELSE(Time<2014,Nuclear_capacity_under_construction[REGIONS_I],
	IF_THEN_ELSE(selection_of_nuclear_scenario[REGIONS_I]=2, 0,
	IF_THEN_ELSE(selection_of_nuclear_scenario[REGIONS_I]=4, 0, Decom_nuclear[REGIONS_I]\
		*replacement_rate_nuclear[REGIONS_I
	])))*CF_limit_nuclear[REGIONS_I]
	~	TW
	~<[VIEW]>: energy-electricity.nuclear_generation <[DESCRIPTION]>:	It is assumed that the step of planning of replaced infraestructure can be \
		done while the infraestructure to be replaced is still under operation.
	|

replacement_rate_nuclear[REGIONS_I]=
	IF_THEN_ELSE(Control_exogenous_new_nuclear_vs_demand[REGIONS_I]>0 :AND: nuclear_overcapacity\
		[REGIONS_I]=0,1,0)
	~	Dmnl
	~<[VIEW]>: energy-electricity.nuclear_generation <[DESCRIPTION]>:	Replacement rate of infrastructure: by default all decommissioned capacity \
		is replaced (=1). In the case that the total electricity is already \
		supplied by nuclear the replacement is stopped.
	|

replacement_rate_PHS[REGIONS_I]=
	IF_THEN_ELSE("%_PHS_overcapacity_vs_potential"[REGIONS_I]<=100 :AND: PHS_overcapacity\
		[REGIONS_I]=0,1,0)
	~	Dmnl
	~<[VIEW]>: energy-electricity-PHS_generation <[DESCRIPTION]>:	Replacement rate of infrastructure: by default all decommissioned capacity \
		is replaced (=1). In the case of overcapacity in relation to the \
		potential, the replacement is eliminated.
	|

replacement_rate_RES_elec[RES_elec, REGIONS_I]=
	MIN(1 , IF_THEN_ELSE("%_RES_elec_overcapacity_vs_max_potential"[RES_elec,REGIONS_I]<=\
		100 :AND: Control_exogenous_new_RES_elec_vs_demand
	[REGIONS_I]>0 :AND: RES_elec_tot_overcapacity[REGIONS_I]=0,1,0)*SMOOTH_EROI_allocation_rule_per_RES_elec\
		[RES_elec,REGIONS_I])*constraint_elec_storage_availability[REGIONS_I,RES_elec]
	~	Dmnl
	~<[VIEW]>: energy-electricity-ELEC RES.generation <[DESCRIPTION]>:	Replacement rate of infrastructure: by default all decommissioned capacity \
		is replaced (=1). In the case of overcapacity in relation to the potential \
		of each RES elec, the replacement of that RES elec is eliminated. In the \
		case that the total electricity is already supplied by RES elec, the \
		replacement is also stopped.
	|

"replacement_rate_RES_heat-com"[RES_heat, REGIONS_I]=
	IF_THEN_ELSE("%_RES_heat_overcapacity_vs_potential"[RES_heat,REGIONS_I]<=100 :AND: "Control_exogenous_new_RES_heat-com_vs_demand"\
		[REGIONS_I],1,0)
	~	Dmnl
	~<[VIEW]>: energy-heat.commercial_generation_RES <[DESCRIPTION]>:	Replacement rate of infrastructure: by default all decommissioned capacity \
		is replaced (=1). In the case of overcapacity in relation to the potential \
		of each RES heat, the replacement of that RES elec is eliminated. In the \
		case that the total heat-com is already supplied by RES heat-com, the \
		replacement is also stopped.
	|

"replacement_rate_RES_heat-nc"[RES_heat, REGIONS_I]=
	IF_THEN_ELSE("%_RES_heat_overcapacity_vs_potential"[RES_heat,REGIONS_I]<=100 :AND: "Control_exogenous_new_RES_heat-nc_vs_demand"\
		[REGIONS_I],1,0)
	~	Dmnl
	~<[VIEW]>: energy-heat.non_commercial_generation_RES <[DESCRIPTION]>:	Replacement rate of infrastructure: by default all decommissioned capacity \
		is replaced (=1). In the case of overcapacity in relation to the potential \
		of each RES heat, the replacement of that RES elec is eliminated. In the \
		case that the total heat-nc is already supplied by RES heat-nc, the \
		replacement is also stopped.
	|

"replacement_RES_for_heat-com_TW"[RES_heat, REGIONS_I]=
