A_Matrix[REGIONS_I,sectors,sectors1]=
	MIN(MAX(IF_THEN_ELSE(Time<2015, Historic_A_Matrix[sectors,sectors1],A_Matrix_evolution\
		[REGIONS_I,sectors,sectors1]),0),1)
	~	Dmnl
	~<[VIEW]>: economy.structural_change <[DESCRIPTION]>:	A Matrix used to calculated Leontief Matrix. (1995-2009) historic data \
		from WIOD database and (2009-) estimated in the model following different \
		options.
	|

A_Matrix_delayed[REGIONS_I,sectors,sectors1,DTime_Step]= DELAY_FIXED (
	A_Matrix[REGIONS_I,sectors,sectors1], TIME_STEP, 0)
	~	Dmnl
	~<[VIEW]>: economy.structural_change <[DESCRIPTION]>:	A Matrix delayed one time step.
	|

A_Matrix_evolution[REGIONS_I,sectors,sectors1]=
	IF_THEN_ELSE(Sum_columns_first_step[REGIONS_I,sectors]>=Maximum_Sum_columns[REGIONS_I\
		,sectors],1*A_Matrix_delayed[REGIONS_I,sectors,sectors1,DTime_Step],A_Matrix_evolution_first_step
	[REGIONS_I,sectors,sectors1])
	~	Dmnl
	~<[VIEW]>: economy.structural_change <[DESCRIPTION]>:	A Matrix evolution estimated in the model following different options.
	|

A_Matrix_evolution_first_step[REGIONS_I,sectors,sectors1]=
	IF_THEN_ELSE(Time<INITIAL_YEAR_A_MATRIX_EVOLUTION[REGIONS_I],1*A_Matrix_delayed[REGIONS_I\
		,sectors,sectors1,DTime_Step],
	IF_THEN_ELSE(Time<FINAL_YEAR_TARGET_A_MATRIX_EVOLUTION[REGIONS_I],A_matrix_evolution_multipliers\
		[REGIONS_I,sectors,sectors1]*A_Matrix_delayed[REGIONS_I,sectors,sectors1,DTime_Step\
		],1*A_Matrix_delayed[REGIONS_I,sectors,sectors1,DTime_Step]))
	~	Dmnl
	~<[VIEW]>: economy.structural_change <[DESCRIPTION]>:	First step in A Matrix evolution estimated in the model following \
		different options.
	|

A_matrix_evolution_multipliers[REGIONS_I,sectors,sectors1]=
	IF_THEN_ELSE(SWITCH_SELECTION_A_MATRIX_EVOLUTION_SP[REGIONS_I]=0,Constant_A_Matrix_evolution_multiplier\
		[sectors,sectors1
	],
	IF_THEN_ELSE(SWITCH_SELECTION_A_MATRIX_EVOLUTION_SP[REGIONS_I]=1,IF_THEN_ELSE((Objective_A_Matrix_regions_selection\
		[REGIONS_I
	,sectors,sectors1]-A_MATRIX_HISTORIC[sectors,sectors1,year2009])/A_MATRIX_HISTORIC[sectors\
		,sectors1,year2009]>0,1+(Objective_A_Matrix_regions_selection
	[REGIONS_I,sectors,sectors1]-A_MATRIX_HISTORIC[sectors,sectors1,year2009])/A_MATRIX_HISTORIC\
		[sectors,sectors1,year2009],1-ABS((Objective_A_Matrix_regions_selection
	[REGIONS_I,sectors,sectors1]-A_MATRIX_HISTORIC[sectors,sectors1,year2009])/A_MATRIX_HISTORIC\
		[sectors,sectors1,year2009]))^(TIME_STEP/(FINAL_YEAR_TARGET_A_MATRIX_EVOLUTION
	[REGIONS_I]-INITIAL_YEAR_A_MATRIX_EVOLUTION[REGIONS_I])),
	IF_THEN_ELSE(SWITCH_SELECTION_A_MATRIX_EVOLUTION_SP[REGIONS_I]=2,IF_THEN_ELSE(((A_MATRIX_HISTORIC\
		[sectors,sectors1,year2009]-A_MATRIX_HISTORIC[sectors,sectors1,year1995])/
	A_MATRIX_HISTORIC[sectors,sectors1,year1995])>0,1+((A_MATRIX_HISTORIC[sectors,sectors1\
		,year2009]-A_MATRIX_HISTORIC[sectors,sectors1,year1995])/
	A_MATRIX_HISTORIC[sectors,sectors1,year1995]),1-(ABS((A_MATRIX_HISTORIC[sectors,sectors1\
		,year2009]-A_MATRIX_HISTORIC[sectors,sectors1,year1995])/A_MATRIX_HISTORIC[sectors,\
		sectors1,year1995])))^(TIME_STEP/(2009-1995+1)),Constant_A_Matrix_evolution_multiplier\
		[sectors,sectors1])))
	~	Dmnl
	~<[VIEW]>: economy.structural_change <[DESCRIPTION]>:	Switch:
		0 for constant
		1 for linear projection towards objective matrix
		2 for historical projection
	|

