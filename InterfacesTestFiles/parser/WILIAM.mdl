	IF_THEN_ELSE(Time<1997, A_MATRIX_HISTORIC[sectors,sectors1,year1996],
	IF_THEN_ELSE(Time<1998, A_MATRIX_HISTORIC[sectors,sectors1,year1997],
	IF_THEN_ELSE(Time<1999, A_MATRIX_HISTORIC[sectors,sectors1,year1998],
	IF_THEN_ELSE(Time<2000, A_MATRIX_HISTORIC[sectors,sectors1,year1999],
