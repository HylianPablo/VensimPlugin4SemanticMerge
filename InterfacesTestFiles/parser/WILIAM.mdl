GHG_endongenous_emissions_by_households[REGIONS_I]=
	((Households_final_energy_demand[REGIONS_I,electricity]-Transport_households_final_energy_demand\
		[REGIONS_I,electricity])*ratio_GHG_emissions_by_FE
	[REGIONS_I,electricity]*EROI_FC_system_from_2015[REGIONS_I])+
	((Households_final_energy_demand[REGIONS_I,heat]-Transport_households_final_energy_demand\
		[REGIONS_I,heat])*ratio_GHG_emissions_by_FE[REGIONS_I,heat]*EROI_FC_system_from_2015\
		[REGIONS_I]*(1+ratio_FED_for_heat_nc_vs_FED_for_heat_com[REGIONS_I]))+
	((Households_final_energy_demand[REGIONS_I,liquids]-Transport_households_final_energy_demand\
		[REGIONS_I,liquids])*ratio_GHG_emissions_by_FE
	[REGIONS_I,liquids]*EROI_FC_system_from_2015[REGIONS_I]*(1-share_FEH_over_FED_by_final_energy\
		[liquids,REGIONS_I]))+
	((Households_final_energy_demand[REGIONS_I,gases]-Transport_households_final_energy_demand\
		[REGIONS_I,gases])*ratio_GHG_emissions_by_FE
	[REGIONS_I,gases]*EROI_FC_system_from_2015[REGIONS_I]*(1-share_FEH_over_FED_by_final_energy\
		[gases,REGIONS_I]))+
	((Households_final_energy_demand[REGIONS_I,solids]-Transport_households_final_energy_demand\
		[REGIONS_I,solids])*ratio_GHG_emissions_by_FE
	[REGIONS_I,solids]*EROI_FC_system_from_2015[REGIONS_I]*(1-share_FEH_over_FED_by_final_energy\
		[solids,REGIONS_I]))
	~	GTCO2e/Year
