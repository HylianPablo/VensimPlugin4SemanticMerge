HFC_in_Atm[HFC_type]= INTEG (
	Global_HFC_emissions[HFC_type]-HFC_uptake[HFC_type],
		Initial_HFC[HFC_type])
	~	tons [2.5924e-43,?]
	~<[VIEW]>: climate.other_GHG_cycles <[DESCRIPTION]>:		|

HFC_molar_mass[HFC_type]=
	GET_DIRECT_CONSTANTS('/model_parameters/climate/climate.xlsx', 'World', 'HFC_molar_mass*'\
		)
	~	g/mole
	~<[VIEW]>: climate.exogenous_inputs <[DESCRIPTION]>:	http://www.qc.ec.gc.ca/dpe/publication/enjeux_ges/hfc134a_a.html
	|

HFC_radiative_efficiency[HFC_type]=
	GET_DIRECT_CONSTANTS('/model_parameters/climate/climate.xlsx', 'World', 'HFC_radiative_efficiency*'\
		)
	~	watt/(ppb*meter*meter)
	~<[VIEW]>: climate.exogenous_inputs <[DESCRIPTION]>:	From AR5 WG1 Chapter 8.  Table 8.A.1. Lifetimes, Radiative Efficiencies \
		and Metric Values
	|

HFC_RF[HFC_type]=
	(HFC_atm_conc[HFC_type]-Preindustrial_HFC_conc)*HFC_radiative_efficiency[HFC_type]/ppt_PER_ppb
	~	watt/(meter*meter)
	~<[VIEW]>: climate.other_GHG_cycles <[DESCRIPTION]>:	HFC radiative forcing.
	|

HFC_RF_total=
	SUM(HFC_RF[HFC_type!])
	~	watt/(meter*meter)
	~<[VIEW]>: climate.radiative_forcing <[DESCRIPTION]>:	The sum of the RFs of the individual HFC types.
	|

HFC_type:
	HFC134a	,
	HFC23	,
	HFC32	,
	HFC125	,
	HFC143a	,
	HFC152a	,
	HFC227ea	,
	HFC245ca	,
	HFC4310mee
	~	
	~	Hydrofluorocarbons.
	|

HFC_uptake[HFC_type]=
	HFC_in_Atm[HFC_type]/Time_Const_for_HFC[HFC_type]
	~	tons/Year
	~<[VIEW]>: climate.other_GHG_cycles <[DESCRIPTION]>:		|

