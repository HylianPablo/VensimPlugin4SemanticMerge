	~	Tdollars
	~<[VIEW]>: intermodule_consistency.confrontation_demand_vs_l <[DESCRIPTION]>:		|

GDPpc[REGIONS_I]=
	GDP[REGIONS_I]*MATRIX_UNIT_PREFIXES[tera,BASE_UNIT]/population[REGIONS_I]
	~	$/people
	~<[VIEW]>: economy.indicators <[DESCRIPTION]>:	GDP per capita (1995T$ per capita).
	|

GDPpc_ANNUAL_GROWTH_SSP2_LT(
	GET_DIRECT_LOOKUPS('/model_parameters/economy/economy.xlsx', 'World', 'time_index_GDPpc'\
		, 'SSP2_GDP'))
	~	Dmnl
	~<[VIEW]>: economy.exogenous_inputs <[DESCRIPTION]>:		|

GDPpc_asymptote_variation[REGIONS_I]=
	IF_THEN_ELSE(Time<STARTING_YEAR_FOR_GDPpc_VARIATION_POLICY_SP[REGIONS_I],POLICY_TIMESERIES_GDPpc_GROWTH_RATE_SP\
		[REGIONS_I],
	
	ZIDZ( POLICY_GDPpc_ASYMPTOTE_SP[REGIONS_I]-expected_GDPpc[REGIONS_I] ,difference_target_GDPpc_asymptote_vs_starting_year\
		[REGIONS_I]))
	~	
	~<[VIEW]>: economy.expected_GDPpc <[DESCRIPTION]>:	IF_THEN_ELSE(Time<STARTING_YEAR_FOR_POPULATION_VARIATION_POLICY_SP[REGIONS_I],POLICY_\
		TIMESERIES_POPULATION_VARIATION_RATE_SP
		[REGIONS_I],((POLICY_POPULATION_TARGET_ASYMPTOTE_SP[REGIONS_I]*MATRIX_UNIT_PREFIXES[m\
		ega,BASE_UNIT])-population[REGIONS_I])/difference_target_population_asympto\
		te_vs_starting_year
		[REGIONS_I])
	|

GDPpc_INITIAL_YEAR=
	GET_DIRECT_CONSTANTS('/model_parameters/economy/economy.xlsx', 'World', 'GDPpc_initial_year'\
		)
	~	$/person
	~<[VIEW]>: economy.exogenous_inputs <[DESCRIPTION]>:	Initial value of GDPpc. Data from WIOD and World Bank
	|

"GDPpc_t-1"[REGIONS_I]=
	SMOOTH_N(GDPpc[REGIONS_I], 1, 5107, 12)
	~	Tdollars/Year
	~<[VIEW]>: economy.indicators <[DESCRIPTION]>:	GDPpc projection delayed 1 year.
	|

GDPpc_variation[REGIONS_I]=
	IF_THEN_ELSE(Time<2014, VARIATION_HISTORIC_GDPpc,
	IF_THEN_ELSE(Time<STARTING_YEAR_FOR_GDPpc_VARIATION_POLICY_SP[REGIONS_I], POLICY_TIMESERIES_GDPpc_GROWTH_RATE_SP\
		[REGIONS_I
	]*expected_GDPpc[REGIONS_I],
	
	IF_THEN_ELSE(SWITCH_SELECT_GDPpc_EVOLUTION_SP[REGIONS_I]=0, GDPpc_ANNUAL_GROWTH_SSP2_LT\
		(Time)*expected_GDPpc[REGIONS_I]
	,
	
	IF_THEN_ELSE(SWITCH_SELECT_GDPpc_EVOLUTION_SP[REGIONS_I]=1, POLICY_TIMESERIES_GDPpc_GROWTH_RATE_SP\
		[REGIONS_I]*expected_GDPpc
	[REGIONS_I],
	
	IF_THEN_ELSE(SWITCH_SELECT_GDPpc_EVOLUTION_SP[REGIONS_I]=2,POLICY_CONSTANT_GDPpc_VARIATION_SP\
		[REGIONS_I]*expected_GDPpc
	[REGIONS_I],
	smooth_expected_variation_GDPpc[REGIONS_I])))))
	~	$/person
	~<[VIEW]>: economy.expected_GDPpc <[DESCRIPTION]>:	Expected variation of GDP per capita.
		
		IF_THEN_ELSE(Time>2016:AND:SWITCH_SELECT_GDPpc_EVOLUTION_SP[REGIONS_I]=3,smooth_expec\
		ted_variation_GDPpc[REGIONS_I],expected_variation_GDPpc_per_scen
		[REGIONS_I])
	|

