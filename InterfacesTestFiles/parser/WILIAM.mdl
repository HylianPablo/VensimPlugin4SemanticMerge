		[RES_elec,REGIONS_I])<100,100,-1+ZIDZ(Available_generation_RES_elec_TWh[RES_elec,REGIONS_I\
		]*100, max_potential_RES_elec_TWh[RES_elec,REGIONS_I]))
	~	percent
