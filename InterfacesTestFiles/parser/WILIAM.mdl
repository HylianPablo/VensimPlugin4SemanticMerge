		[REGIONS_I], (Max_PEavail_potential_biofuels_marginal_lands
	[REGIONS_I]-PEavail_biofuels_land_marg_EJ[REGIONS_I])/Max_PEavail_potential_biofuels_marginal_lands\
		[REGIONS_I], 0)
	~	Dmnl
	~<[VIEW]>: energy-RES_bioenergy.biocrops_marginal_land <[DESCRIPTION]>:	Remaining potential available as a fraction of unity.
	|

remaining_potential_elec_storage_by_RES_techn[REGIONS_I, RES_elec]=
	IF_THEN_ELSE(max_capacity_elec_storage[REGIONS_I] >= Total_capacity_elec_storage_TW[\
		REGIONS_I],
	(max_capacity_elec_storage[REGIONS_I]-Total_capacity_elec_storage_TW[REGIONS_I])/max_capacity_elec_storage\
		[
	REGIONS_I], 0)
	~	Dmnl
	~<[VIEW]>: energy-electricity-storage_demand_and_supply <[DESCRIPTION]>:	Remaining potential available as a fraction of unity.
	|

remaining_potential_solar_PV_urban[REGIONS_I]=
	MAX(0, ZIDZ( max_solar_PV_urban[REGIONS_I]/(1/Wh_PER_We) - desired_share_installed_PV_urban_vs_tot_PV\
		[REGIONS_I]*Available_generation_RES_elec_TWh[solar_PV,REGIONS_I] , max_solar_PV_urban\
		[REGIONS_I]/(1/Wh_PER_We) ))
	~	Dmnl
	~<[VIEW]>: energy-electricity-ELEC RES.potentials <[DESCRIPTION]>:	Remaining potential of solar PV in urban areas.
	|

remaining_potential_tot_RES[REGIONS_I]=
	IF_THEN_ELSE(max_potential_tot_RES[REGIONS_I] > TFES_RES[REGIONS_I], (max_potential_tot_RES\
		[REGIONS_I]-TFES_RES[REGIONS_I
	])/max_potential_tot_RES[REGIONS_I], 0)
	~	Dmnl
	~<[VIEW]>: energy.RES_total_potential <[DESCRIPTION]>:	Remaining potential of all renewables (elec, heat, biofuels and biogas - \
		excepting traditional biomass) available as a fraction of unity.
	|

remaining_potential_tot_RES_heat[REGIONS_I]=
	IF_THEN_ELSE(max_PE_potential_tot_RES_heat_EJ[REGIONS_I] > PES_tot_RES_for_heat[REGIONS_I\
		], ZIDZ( max_PE_potential_tot_RES_heat_EJ
	[REGIONS_I]-PES_tot_RES_for_heat[REGIONS_I] , max_PE_potential_tot_RES_heat_EJ[REGIONS_I\
		] ), 0)
	~	Dmnl
	~<[VIEW]>: energy-heat.RES_potential <[DESCRIPTION]>:	Remaining potential available as a fraction of unity.
	|

"remaining_share_transm&distr_elec_losses"[REGIONS_I]=
	("Max_share_transm&distr_elec_losses"-"share_transm&distr_elec_losses"[REGIONS_I])/"Max_share_transm&distr_elec_losses"
	~	Dmnl
	~<[VIEW]>: energy-electriciy.demand <[DESCRIPTION]>:	Remaining share in relation to the assumed maximum transmission and \
		distribution losses.
	|

